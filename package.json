{
  "name": "leet-speak-converter",
  "version": "1.0.1",
  "description": "A smart and simple utility to convert text into leet speak.",
  "main": "main.js",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=0.27.x"
  },
  "scripts": {
    "clean": "rm -r ./dist/*",
    "build": "cross-env NODE_ENV=production babel ./src --out-dir ./dist --source-maps --copy-files",
    "start:cli": "cross-env NODE_ENV=development node ./src/cli.js",
    "start:web": "cross-env NODE_ENV=development gulp dev",
    "test": "cross-env NODE_ENV=test eslint ./src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexdevero/leet-speak-converter.git"
  },
  "keywords": [
    "converter",
    "l33t",
    "leet",
    "language",
    "speak",
    "javascript",
    "open",
    "open-source",
    "source"
  ],
  "author": "Alex Devero <deveroalex@gmail.com> (http://github.com/alexdevero)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexdevero/leet-speak-converter/issues"
  },
  "homepage": "https://github.com/alexdevero/leet-speak-converter#readme",
  "dependencies": {
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-nested": "^2.1.2",
    "postcss-pxtorem": "^4.0.1",
    "postcss-simple-vars": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.18.13",
    "cross-env": "^5.1.1",
    "cssnano": "^3.10.0",
    "eslint": "^4.12.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-csscomb": "^3.0.8",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "gulp-prune": "^0.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sequence": "^0.4.6",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "nodemon": "^1.12.1",
    "postcss-cli": "^4.1.1",
    "require-dir": "^0.3.2"
  }
}
