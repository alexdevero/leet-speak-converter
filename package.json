{
  "name": "leet-speak-encoder",
  "version": "1.0.0",
  "description": "A simple utility to convert text into leet speak.",
  "main": "main.js",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=0.27.x"
  },
  "scripts": {
    "start": "set NODE_ENV=development && node ./src/scripts/main.js",
    "start:cli": "set NODE_ENV=development && node ./src/scripts/cli.js",
    "build": "set NODE_ENV=production && babel ./src/scripts/main.js --out-file ./dist/scripts/main.js",
    "test": "set NODE_ENV=test && eslint ./src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexdevero/leet-speak-converter.git"
  },
  "keywords": [
    "converter",
    "leet",
    "language",
    "speak",
    "javascript",
    "open",
    "open-source",
    "source"
  ],
  "author": "Alex Devero <deveroalex@gmail.com> (http://github.com/alexdevero)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexdevero/leet-speak-converter/issues"
  },
  "homepage": "https://github.com/alexdevero/leet-speak-converter#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.9.0"
  }
}
